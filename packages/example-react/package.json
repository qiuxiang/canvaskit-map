{
  "private": true,
  "name": "@canvaskit-tilemap/example-react",
  "version": "0.0.1",
  "main": "src/main",
  "scripts": {
    "uno": "unocss -o dist/uno.css 'src/**/*.tsx' index.html",
    "esbuild": "esbuild --bundle --tsconfig=./tsconfig.json --minify --external:fs --external:path --outdir=dist src/index.tsx",
    "dev": "(pnpm run uno -w) & pnpm run esbuild --watch --sourcemap --serve --servedir=dist"
  },
  "dependencies": {
    "@canvaskit-tilemap/example": "workspace:~",
    "@canvaskit-tilemap/react": "workspace:~",
    "preact": "^10.17.1",
    "valtio": "^1.11.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@unocss/cli": "^0.55.7",
    "unocss": "^0.55.7"
  }
}
